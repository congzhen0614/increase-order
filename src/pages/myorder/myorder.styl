.solicit-subscription-myorder
  padding-bottom 1rem
  .no-content
    padding-top 3rem
    height 100%
    > img
      display block
      width 2.5rem
      height 2.5rem
      margin 0 auto
    .go-and-see-button
      display block
      width 2rem
      height 0.8rem
      background-color #F8BD48
      border-radius 5px
      font-size 0.34rem
      color #FFFFFF
      line-height 0.8rem
      text-align center
      margin 0 auto
      margin-top 0.8rem
  .order-list:first-child
    margin-top 0
  .order-list
    background-color #F3F4F5
    padding-bottom 0
    padding-top 8px
    .order-title
      position relative
      height 0.88rem
      padding 0 0.2rem
      line-height 0.88rem
      background-color #ffffff
      .order-time
        color #333333
        font-size 0.28rem
      .order-status
        color #333333
        font-weight 600
        font-size 0.28rem
        float right
      .delete-icon
        float right
        display block
        width 0.88rem
        height 0.88rem
        text-align center
        line-height 0.88rem
        margin-right -0.2rem
        > img
          width 0.24rem
          height 0.24rem
      > img
        position absolute
        top 0
        right 0.88rem
        width 1.4rem
        height 1.2rem
    > ul
      margin-left 0.2rem
      .details-list
        position relative
        overflow hidden
        padding 0.3rem 0
        padding-right 0.2rem
        border-bottom 1px solid #E6E6E6
        .details-left
          float left
          width 1.6rem
          height 1.6rem
          overflow hidden
          border-radius 5px
          margin-right 0.2rem
          border 1px solid #D6D6D6
          > img
            display block
            height 100%
            margin 0 auto
        .details-right
          .details-price
            float right
            color #333333
            font-size 0.2rem
            margin-left 0.4rem
            .big
              font-size 0.32rem
              vertical-align baseline
          .details-tital
            color #333333
            font-size 0.3rem
            margin-bottom 0.2rem
            text-align justify
            overflow hidden
            text-overflow ellipsis
            display -webkit-box
            -webkit-line-clamp 2
            -webkit-box-orient vertical
          .details-fee
            color #9C9890
            font-size 0.24rem
          .details-quantity
            position absolute
            right 0.2rem
            bottom 0.3rem
            display block
            height 0.4rem
            line-height 0.4rem
            font-size 0.28rem
            color #FFFFFF
            padding 0 0.16rem
            border-radius 0.4rem
            background-color #F8BD48
      .details-list:last-child
        border none
    .order-total-border
      border-bottom 1px solid #D6D6D6
    .order-total
      height 0.88rem
      overflow hidden
      padding 0 0.2rem
      line-height 0.88rem
      background-color #ffffff
      .total-price
        float right
        color #333333
        font-size 0.2rem
        .big
          font-size 0.36rem
          vertical-align baseline
        .medium
          font-size 0.28rem
          vertical-align baseline
      .total-quantity
        font-size 0.28rem
    .order-button
      width 100%
      height 1rem
      overflow hidden
      line-height 1rem
      padding 0.16rem 0
      background-color #ffffff
      > span
        float right
        display block
        color #F8BD48
        height 0.68rem
        padding 0 0.34rem
        line-height 0.64rem
        border-radius 1rem
        border 1px solid #F8BD48
        margin-right 0.2rem
