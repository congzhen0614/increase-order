.success
  position relative
  padding-top 1px
  width 100%
  height 100%
  background-image url("../../../assets/pay/success.png")
  background-repeat no-repeat
  background-size 100% auto
  .close
    font-size 0.28rem
    color #806226
    margin 0.4rem
    float right
  .amount
    margin-top 1.5rem
    text-align center
    font-size 0.24rem
    color #523F1A
    .big
      font-size 0.8rem
      vertical-align baseline
  .paysuccess
    text-align center
    margin-top 0.1rem
    font-size 0.28rem
    color #72A620
  > img
    display block
  .checkorder
    position absolute
    width 2.34rem
    left 50%
    bottom 4rem
    margin-left -1.17rem
  .spyp
    font-size 0.36rem
    padding 0 0.4rem
    text-align center
    margin-top 3rem
    color #523F1A
  .download
    width 2.34rem
    margin 0 auto
    margin-top 0.68rem
  .logo
    position absolute
    bottom 1.12rem
    left 50%
    width 0.8rem
    margin-left -0.4rem
  .font
    position absolute
    bottom 0.6rem
    left 50%
    width 2.86rem
    margin-left -1.43rem
